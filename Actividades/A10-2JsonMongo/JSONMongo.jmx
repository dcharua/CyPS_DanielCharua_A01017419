            <stringProp name="parameters"></stringProp>
            <stringProp name="script">import groovy.json.JsonSlurper;
import com.mongodb.*;


JsonSlurper s = new JsonSlurper();

MongoClient cli = new MongoClient(&quot;localhost&quot;, 27017);
DB mydb = cli.getDB(&quot;usersJ&quot;);
DBCollection nombres = mydb.getCollection(&quot;nombres&quot;);
WriteResult writer;
def resultado = s.parseText(prev.getResponseDataAsString());
var.putObject(resultado);
resultado.name.each{
	BasicDBObject bdbo = new BasicDBObject();
	OUT.println(&quot;$it&quot;);
	bdbo.append(&quot;nombre&quot;, &quot;$it&quot;.toString());
	
	writer = nombres.insert(bdbo, WriteConcern.ACKNOWLEDGED);
	if(writer.wasAcknowledged()){
    		log.println(&quot;Se escribio el objeto en la BD&quot;);
	} else {
	    log.println(&quot;NO se escribio el objeto en la BD&quot;);
	}
}
</stringProp>
